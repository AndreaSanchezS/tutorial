<mvc:View
    controllerName="accenture.tutorial1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>

    <Page id="page" title="{i18n>title}">
        <content>
            <Button text="Hello World!" press="onSayHello"></Button>
             <ComboBox id="city-combobox" items="{CitiesModel>/}" selectedKey="{ToolModel>/selectedCity}">
                    <core:Item text="{CitiesModel>name}" key="{CitiesModel>name}" />
             </ComboBox>
             <Button text="Navigate to View 2" press="onNavigateToView2"></Button>
             <Button text="Navigate to View 3" press="onNavigateToView3"></Button>
        </content>
    </Page>

</mvc:View>